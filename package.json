{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint src --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^4.16.6",
    "lodash": "^4.17.21",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.3.0",
    "babel-eslint": "^10.1.0",
    "less": "^4.1.1",
    "tailwindcss": "^2.2.4",
    "vite": "^2.4.0",
    "vite-aliases": "^0.8.2",
    "vite-plugin-babel-import": "^2.0.5",
    "vite-plugin-imp": "^2.0.7"
  }
}
